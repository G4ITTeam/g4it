        "@angular/platform-browser": "^18.2.13",
        "@angular/platform-browser-dynamic": "^18.2.13",
        "@angular/router": "^18.2.13",
        "@ngneat/elf": "^2.5.1",
        "@ngneat/elf-devtools": "^1.3.0",
        "@ngneat/elf-entities": "^5.0.2",
        "@ngneat/elf-persist-state": "^1.2.1",
        "@ngx-translate/core": "^16.0.3",
