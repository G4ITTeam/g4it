{
    "name": "frontend",
    "version": "2.1.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:overgreen": "ng build --base-href /overgreen/ --deploy-url /overgreen/",
        "watch": "ng build --watch --configuration development",
        "e2e": "ng e2e",
        "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
        "test": "ng test --code-coverage --configuration=local --include='**/*.spec.ts'",
        "test:ci": "ng test --configuration=ci",
        "generate": "ng generate",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run -e mode=one-report"
    },
    "private": true,
